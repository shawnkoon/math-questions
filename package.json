  "main": "./dist/index.js",
  "scripts": {
    "build": "rm -rf ./dist && babel src -d dist --presets es2015",
    "test": "mocha ./src/**/*.test.js --require babel-register --reporter spec",
    "lint": "eslint src/**/*.js",
    "clean:install": "rm -rf node_modules package-lock.json && npm i"
